// Temporarily just a list of key/val

String.Map
: ∀(A: *) *
= (List.Map String)

String.Map.get
: ∀(A: *)
  ∀(key: String)
  ∀(map: (String.Map A))
  (Maybe A)
= λA λkey λmap
  (List.Map.get String A String.equal key map)

String.Map.set
: ∀(A: *)
  ∀(key: String)
  ∀(val: A)
  ∀(map: (String.Map A))
  (String.Map A)
= λA λkey λval λmap
  (List.Map.set String A key val map)

String.Map.new
: ∀(V: *)
  (String.Map V)
= λV (List.Map.new String V)
